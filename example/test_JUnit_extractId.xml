<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent" tools:context=".MainActivity">

	<IdBasic1 android:id="@+id/basicId"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:text="TextView1Text"
		app:layout_IdBottom_toTopOf="@+id/textView2"
		app:layout_IdLeft_toLeftOf="parent"
		app:layout_IdRight_toRightOf="parent"
		app:layout_IdTop_toTopOf="parent" />

	<IdEmptyId android:id="@+id/"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:text="TextView1Text"
		app:layout_IdBottom_toTopOf="@+id/textView2"
		app:layout_IdLeft_toLeftOf="parent"
		app:layout_IdRight_toRightOf="parent"
		app:layout_IdTop_toTopOf="parent" />

	<IdNullId android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:text="TextView1Text"
		app:layout_IdBottom_toTopOf="@+id/textView2"
		app:layout_IdLeft_toLeftOf="parent"
		app:layout_IdRight_toRightOf="parent"
		app:layout_IdTop_toTopOf="parent" />



</androidx.constraintlayout.widget.ConstraintLayout>