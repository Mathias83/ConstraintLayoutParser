<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent" tools:context=".MainActivity">

	<ConstraintBasic1 android:id="@+id/constraint"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:text="TextView1Text"
		app:layout_constraintBottom_toTopOf="@+id/textView2"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toTopOf="parent" />

	<ConstraintBasic2 android:id="@+id/constraint"
		android:layout_width="match_parent"
		android:layout_height="match_parent" android:text="TextView1Text"
		app:layout_constraintBottom_toTopOf="@+id/textView2"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toTopOf="parent" />

	<ConstraintNoID android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:text="TextView1Text"
		app:layout_constraintBottom_toTopOf="@+id/textView2"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toTopOf="parent" />

	<ConstraintEmptyID android:id="@+id/"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:text="TextView1Text"
		app:layout_constraintBottom_toTopOf="@+id/textView2"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toTopOf="parent" />

	<ConstraintNoConstraintWidth
		android:id="@+id/textView1" android:layout_height="wrap_content"
		android:text="TextView1Text"
		app:layout_constraintBottom_toTopOf="@+id/textView2"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toTopOf="parent" />

	<ConstraintNoConstraintHeight
		android:id="@+id/textView1" android:layout_width="wrap_content"
		android:text="TextView1Text"
		app:layout_constraintBottom_toTopOf="@+id/textView2"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toTopOf="parent" />

	<ConstraintBasic android:id="@+id/textView1"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:text="TextView1Text"
		app:layout_constraintBottom_toTopOf="@+id/textView2"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>